{
  "name": "spaceApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx,",
    "start": "react-native start --reset-cache",
    "postinstall": "patch-package",
    "test": "jest",
    "type-check": "tsc",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "format": "prettier --write .",
    "depcheck": "depcheck",
    "prepare": "husky install",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "androidbuild": "react-native run-android --variant=release --verbose",
    "android": "rm -rf node_modules/ && yarn && cd android && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native start --reset-cache",
    "ios": "rm -rf node_modules/ && yarn && cd ios && rm -rf Pods/ build/ Podfile.lock && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update && cd .. && npx react-native start --reset-cache",
    "a": "yarn && cd android && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native start --reset-cache",
    "i": "yarn && cd ios && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update && cd .. && npx react-native start --reset-cache",
    "aabi": "cd android && adb uninstall com.spaceApp && cd app && rm -rf .cxx && cd .. && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native run-android --active-arch-only"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-firebase/analytics": "^18.5.0",
    "@react-native-firebase/app": "^18.5.0",
    "@react-native-firebase/remote-config": "^18.5.0",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.18",
    "@reduxjs/toolkit": "^1.9.7",
    "i18next": "^23.5.1",
    "jail-monkey": "^2.8.0",
    "lodash": "^4.17.21",
    "lottie-react-native": "^6.3.1",
    "react": "^18.2.0",
    "react-i18next": "^13.3.0",
    "react-native": "^0.72.6",
    "react-native-device-info": "^10.11.0",
    "react-native-error-boundary": "^1.2.3",
    "react-native-gesture-handler": "^2.13.2",
    "react-native-mmkv": "^2.10.2",
    "react-native-mmkv-flipper-plugin": "^1.0.0",
    "react-native-performance": "^5.1.0",
    "react-native-performance-flipper-reporter": "^5.0.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^13.14.0",
    "react-native-webp-format": "^1.2.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@react-navigation/devtools": "^6.0.19",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.5",
    "@types/lodash.memoize": "^4.1.7",
    "@types/metro-config": "^0.76.3",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-native": "^0.72.3",
    "@types/react-test-renderer": "^18.0.3",
    "@types/styled-components": "^5.1.28",
    "@types/styled-components-react-native": "^5.2.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "depcheck": "^1.4.6",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ft-flow": "^3.0.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.3",
    "react-native-flipper": "^0.212.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "^18.2.0",
    "redux-flipper": "^2.0.2",
    "stylelint": "^15.10.3",
    "stylelint-react-native": "^2.6.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
}
