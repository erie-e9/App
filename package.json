{
  "name": "spaceApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx,",
    "prestart": "nvm use 18.19.1",
    "start": "react-native start --reset-cache",
    "postinstall": "patch-package",
    "test": "jest",
    "type-check": "tsc",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "format": "prettier --write .",
    "depcheck": "depcheck",
    "prepare": "husky install",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "androidbuild": "react-native run-android --variant=release --verbose",
    "android": "rm -rf node_modules/ && rm -rf yarn.lock && yarn && cd android && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native@latest start --reset-cache",
    "ios": "rm -rf node_modules/ && rm -rf yarn.lock && yarn && cd ios && rm -rf Pods/ build/ Podfile.lock && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update && cd .. && npx react-native@latest start --reset-cache",
    "a": "yarn && cd android && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native@latest start --reset-cache",
    "i": "yarn && cd ios && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update && cd .. && npx react-native@latest start --reset-cache",
    "aabi": "cd android && adb uninstall com.spaceApp && cd app && rm -rf .cxx && cd .. && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native@latest run-android --active-arch-only"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-firebase/analytics": "^19.2.2",
    "@react-native-firebase/app": "^19.2.2",
    "@react-native-firebase/app-check": "^19.2.2",
    "@react-native-firebase/remote-config": "^19.2.2",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.3",
    "@shopify/flash-list": "^1.6.4",
    "@shopify/react-native-skia": "^1.2.3",
    "i18next": "^23.11.2",
    "jail-monkey": "^2.8.0",
    "lodash": "^4.17.21",
    "lottie-ios": "4.4.1",
    "lottie-react-native": "^6.7.2",
    "react": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.7",
    "react-native-device-info": "^10.13.1",
    "react-native-error-boundary": "^1.2.4",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keychain": "8.1.2",
    "react-native-lottie-splash-screen": "^1.1.2",
    "react-native-mmkv": "^2.12.2",
